{
  "name": "dudebank",
  "module": "index.ts",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "bun --watch run src/index.ts",
    "build": "bun build src/index.ts --outdir dist --target node",
    "start": "bun run dist/index.js",
    "docker:build": "docker compose -f docker-compose.yml build",
    "docker:up": "docker compose -f docker-compose.yml up",
    "docker:down": "docker compose -f docker-compose.yml down",
    "docker:clean": "./scripts/docker-clean-start.sh",
    "load-test": "k6 run rinha-test/rinha.js"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {
    "@elysiajs/cors": "^1.1.0",
    "elysia": "^1.1.0",
    "ioredis": "^5.4.1",
    "pg": "^8.11.5",
    "zod": "^4.0.5"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/pg": "^8.11.6"
  }
}
