{
  "name": "dudebank",
  "module": "index.ts",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "bun --watch run src/index.ts",
    "build": "bun build src/index.ts --outdir dist --target node",
    "start": "bun run dist/index.js",
    "docker:dev": "./scripts/dev.sh",
    "docker:prod": "./scripts/prod.sh",
    "test:local": "./scripts/test.sh",
    "build:dev": "docker compose -f docker-compose.yml -f docker-compose.dev.yml build",
    "build:prod": "docker compose -f docker-compose.yml -f docker-compose.prod.yml build",
    "up:dev": "docker compose -f docker-compose.yml -f docker-compose.dev.yml up",
    "up:prod": "docker compose -f docker-compose.yml -f docker-compose.prod.yml up",
    "down:dev": "docker compose -f docker-compose.yml -f docker-compose.dev.yml down",
    "down:prod": "docker compose -f docker-compose.yml -f docker-compose.prod.yml down",
    "load-test": "k6 run rinha-test/rinha.js"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {
    "elysia": "^1.1.0",
    "@elysiajs/cors": "^1.1.0",
    "pg": "^8.11.5",
    "ioredis": "^5.4.1"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/pg": "^8.11.6"
  }
}
